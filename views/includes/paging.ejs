<div class="mt-3 text-xs text-right">
    <nav aria-label="Page navigation example">
        <ul class="inline-flex items-center -space-x-px">
            <% if(pagination.curSet > 1  ) { %>
            <li>
                <a href="javascript:void(0)" onclick="$('#pageIndex').val('<%=pagination.startPage -1 %>'); fnSearch();" class="block py-2 px-3 ml-0 leading-tight text-gray-500 rounded-l-lg">
                    <span class="sr-only">Previous</span>
                    <svg class="w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M12.707 5.293a1 1 0 010 1.414L9.414 10l3.293 3.293a1 1 0 01-1.414 1.414l-4-4a1 1 0 010-1.414l4-4a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                    </svg>
                </a>
            </li>
            <% }%>
            <%
            for(var i = pagination.startPage; i <= pagination.endPage; i++){
                if(i > pagination.totalPage){
                    break;
                }

            if(i == pagination.pageIndex){
            %>
            <li>
                <a href="javascript:void(0)" class="py-2 px-3 leading-tight"><%=parseInt(i)%></a>
            </li>
            <% } else{  %>
            <li>
                <a href="javascript:void(0)" class="py-2 px-3 leading-tight text-gray-500" onclick="$('#pageIndex').val('<%=i%>'); fnSearch();" ><%=parseInt(i)%></a>
            </li>
            <% }} %>
            <% if(pagination.curSet < pagination.totalSet){  %>
            <li>
                <a href="javascript:void(0)" class="block py-2 px-3 leading-tight text-gray-500 rounded-r-lg" onclick="$('#pageIndex').val('<%=pagination.endPage + 1 %>'); fnSearch();" >
                    <span class="sr-only">Next</span>
                    <svg class="w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd"></path>
                    </svg>
                </a>
            </li>
            <% } %>
        </ul>
    </nav>
</div>