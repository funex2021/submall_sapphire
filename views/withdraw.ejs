    function viewMore() {
        $("#pageIndex").val(Number($("#pageIndex").val()) + 1);
        // var data = { "pageIndex": $("#pageIndex").val() };

        var data = {};
        data.pageIndex = $("#pageIndex").val();

        $.ajax({
            url: '/p/addView',
            dataType: 'json',
            type: 'POST',
            data: data,
            success: function (result) {

                let withdrawList = result.data.withdrawList;
                var pcHtml = '';
                var mobileHtml = '';

                if(withdrawList.length > 0) {
                    
                    for(var i = 0; i < withdrawList.length; i++) {
                        pcHtml += '<tr>';
                        pcHtml +=   '<td class="nftmax-table__column-1 nftmax-table__data-1">';
                        pcHtml +=       '<div class="nftmax-table__product">';
                        pcHtml +=           '<div class="nftmax-table__product-img">'; 
                        pcHtml +=               '<img src="http://nftnting.com/' + withdrawList[i].nftImg + '" alt="#">'; 
                        pcHtml +=           '</div>'; 
                        if(withdrawList[i].confirm_sts_name == '완료') {
                        pcHtml +=    '<a href="javascript:void(0);" onclick="fnGetNftBuyInfo(' + withdrawList[i].seq + ',' + withdrawList[i].index + ');">'
                        }
                        pcHtml +=           '<div class="nftmax-table__product-content">'; 
                        pcHtml +=               '<h4 class="nftmax-table__product-title">'; 
                        pcHtml +=                   withdrawList[i].nftNm; 
                        if(withdrawList[i].nftCnt > 1) {
                        pcHtml +=                   '외 ' + withdrawList[i].nftCnt + '개'; 
                        }
                        pcHtml +=               '</h4>'; 
                        if(withdrawList[i].confirm_sts_name == '완료') {
                        pcHtml +=    '</a>'
                        }
                        pcHtml +=               '<p class="nftmax-table__product-desc">Owned by  <a href="#">CNFT</a></p>'; 
                        pcHtml +=           '</div>'; 
                        pcHtml +=       '</div>'; 
                        pcHtml +=   '</td>'; 
                        pcHtml +=   '<td class="nftmax-table__column-2 nftmax-table__data-2">'; 
                        pcHtml +=       '<div class="nftmax-table__amount nftmax-table__text-one">'; 
                        pcHtml +=           '<span class="nftmax-table__text">'; 
                        pcHtml +=               '<b>' + withdrawList[i].title + '</b>';
                        pcHtml +=           '</span>'; 
                        pcHtml +=       '</div>'; 
                        pcHtml +=   '</td>';
                        pcHtml +=   '<td class="nftmax-table__column-2 nftmax-table__data-2">';
                        pcHtml +=       '<div class="nftmax-table__amount nftmax-table__text-one">';
                        pcHtml +=           '<span class="nftmax-table__text">';
                        pcHtml +=               '<b>' + withdrawList[i].create_dt + '</b>';
                        pcHtml +=           '</span>';
                        pcHtml +=       '</div>';
                        pcHtml +=   '</td>';
                        pcHtml +=   '<td class="nftmax-table__column-3 nftmax-table__data-3">'; 
                        pcHtml +=       '<div class="nftmax-table__amount nftmax-table__text-two">'; 
                        pcHtml +=           '<span class="nftmax-table__text">'; 
                        switch(withdrawList[i].confirm_sts_name) {
                        case '취소' : pcHtml +=   '<div class="nftmax-table__status nftmax-rbcolor">취소</div>'; break;
                        case '대기' : pcHtml +=   '<div class="nftmax-table__status nftmax-sbcolor">대기</div>'; break;
                        case '완료' : pcHtml +=   '<div class="nftmax-table__status nftmax-gbcolor">승인</div>'; break; 
                        default: break;
                        }
                        pcHtml +=           '</span>'; 
                        pcHtml +=       '</div>'; 
                        pcHtml +=   '</td>'; 
                        pcHtml +=   '<td class="nftmax-table__column-4 nftmax-table__data-4">'; 
                        pcHtml +=   '<p class="nftmax-table__text nftmax-table__up-down nftmax-rcolor">' + parseInt(withdrawList[i].buy_num).toLocaleString('ko-KR') + ' KRW</p>'; 
                        pcHtml +=   '</td>'; 
                        pcHtml +=   '<td class="nftmax-table__column-5 nftmax-table__data-5">'; 
                        if(withdrawList[i].confirm_sts_name == '대기') {
                            pcHtml +=   '<a href="javascript:void(0);" class="nftmax-btn trs-white bl-color"'; 
                            pcHtml +=   'onclick="chkAcc(' + parseInt(withdrawList[i].buy_num).toLocaleString('ko-KR') + ',' + $("#nftBankNm").val() + ',' + $("#nftBankAcc").val() + ',' + $("#nftAccNm").val()+ ');">계좌 확인</a>'; 
                        }
                        pcHtml +=   '</td>'; 
                        pcHtml +=   '<td class="nftmax-table__column-5 nftmax-table__data-5">'; 
                        if(withdrawList[i].confirm_sts_name == '대기') {
                            pcHtml +=       '<a href="javascript:void(0);" class="nftmax-btn nftmax-btn__secondary radius" onclick="buyCancelView(' + withdrawList[i].seq + ');">취소</a>'; 
                        }
                        pcHtml +=   '</td>'; 
                        pcHtml +='</tr>'; 
                        // $("#withdraw_mobile").append(mobileHtml);
                    }
                    $(".nftmax-table__body").append(pcHtml);
                } else {
                   return fnAlertModal('더이상 내역이 없습니다.');
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                // $('.popupWrap').hide();
